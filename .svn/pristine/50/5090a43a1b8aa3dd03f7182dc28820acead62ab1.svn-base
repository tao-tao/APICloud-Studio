.iconsTop li{
    margin-left: 70px;
    width: 100px;
    padding-top: 120px;
    cursor: pointer;
}

#bottomArea{
    line-height: 15px;
    margin-top: 40px;
    background: rgba(172,172,172,0.2);
}

#btmIcons{
    width: 68%;
    margin-left: 16%;
    padding: 44px 0px;
}

#prePage{
    z-index: 9;
    position: absolute;
    left: 40px;
    top: 235px;
    width: 37px;
    height: 37px;
    line-height: 0;
    background: url('images/left_arrow.png') no-repeat;
}
#prePage:hover{
    z-index: 9;
    position: absolute;
    left: 39px;
    top: 235px;
    width: 37px;
    height: 37px;
    line-height: 0;
    background: url('images/left_arrow2.png') no-repeat;
}
#nextPage{
    z-index: 9;
    position: absolute;
    left: 734px;
    top: 235px;
    width: 37px;
    height: 37px;
    line-height: 0;
    background: url('images/right_arrow.png') no-repeat;
}
#nextPage:hover{
    z-index: 9;
    position: absolute;
    left: 735px;
    top: 235px;
    width: 37px;
    height: 37px;
    line-height: 0;
    background: url('images/right_arrow2.png') no-repeat;
}

.linkIndex {padding-left: 20px;}

#btmIcons tr td{cursor: pointer;width: 20%;padding-top: 5px;}

