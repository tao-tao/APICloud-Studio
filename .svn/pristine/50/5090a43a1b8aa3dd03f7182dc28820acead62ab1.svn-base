html, body {
    margin: 0;
    padding: 0;
    background-color: #fff;
    font-family: "黑体";
    width: 780px;
    height: 570px;
    font-size: 14px;
}
.wrap {
    width: 780px;
    height: 480px;
    position: absolute;
    left: 50%;
    top: 50%;
    margin-left: -400px;
    margin-top: -250px;
    text-align: center;
}
.wrap ul{
    -webkit-padding-start:0;
    margin-left: -71px;
}
.wrap span{
    z-index: 3;
    font-family: '微软雅黑';
    font-size: 2.3em;
    color: #24daf2;
    display: block;
    margin-top: 55px;
}
.wrap div{
    text-align: center;
    width: 100%;
}
#startPage1{
    background: url('images/guide1_bg.png') no-repeat;
}
#page1Img{
    margin-top: 34px;
}
#startPage2{
    background: url('images/guide2_bg.png') no-repeat;
}
#page2Img{
    margin-top: 68px;
