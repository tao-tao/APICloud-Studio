# 设置是否使用传统Tab的样式
org.eclipse.ui/SHOW_TRADITIONAL_STYLE_TABS=true
# 设置透视图标题栏停泊位置。默认值为TOP_RIGHT，还可以设置为TOP_LEFT、LEFT
org.eclipse.ui/DOCK_PERSPECTIVE_BAR=topRight
# 设置透视图标题栏是否显示打开其它透视图按钮
org.eclipse.ui/SHOW_OPEN_ON_PERSPECTIVE_BAR=true
# 在透视图标题栏上,显示的透视图
org.eclipse.ui/PERSPECTIVE_BAR_EXTRAS=com.apicloud.navigator.perspective
# 打开窗口默认的透视图
org.eclipse.ui/defaultPerspectiveId=com.apicloud.navigator.perspective
# 设置透视图是否显示标题文本。默认值为true
org.eclipse.ui/SHOW_TEXT_ON_PERSPECTIVE_BAR=true

# 启动时是否显示欢迎画面。默认值为true
#org.eclipse.ui/SHOW_INTRO=false
# 设置启动时在splash的画面中, 是否显示进度条。默认值false
org.eclipse.ui/SHOW_PROGRESS_ON_STARTUP = false
# 是否显示内存情况, 并可进行GC操作
org.eclipse.ui/SHOW_MEMORY_MONITOR= true
# 但很多编辑器重叠时，设置编辑器标题文字的最短长度。默认为8个字符
org.eclipse.ui/EDITOR_MINIMUM_CHARACTERS=12
# 设置编辑器是否启用拼写检查
org.eclipse.ui.editors/spellingEnabled=false

# 视图栏默认是在底部，但我喜欢把它们放在顶部：
#org.eclipse.ui.workbench/VIEW_TAB_POSITION=128

# 新工作区打开时不打开欢迎页面，并且在关闭工作台时不再提示：
org.eclipse.ui.workbench/WELCOME_DIALOG=false
org.eclipse.ui.workbench/EXIT_PROMPT_ON_CLOSE_LAST_WINDOW=false
# 在打开新工程向导所知的透视图时，禁用提示或其他动作：
org.eclipse.ui.workbench/SWITCH_PERSPECTIVE_ON_PROJECT_CREATION=never

#定义另外的默认文本字体，注意：字体属性条目比较特殊，对它的修改不会立即生效。
org.eclipse.ui.workbench/org.eclipse.jface.textfont=1|Consolas|12.0|0|WINDOWS|1|-16|0|0|0|400|0|0|0|0|3|2|1|49|Consolas;
						    

# 设置快捷键配置schema in binding defination:org.eclipse.ui.defaultAcceleratorConfiguration
#org.eclipse.ui/KEY_CONFIGURATION_ID=org.eclipse.ui.defaultAcceleratorConfiguration
# ENABLE_ANIMATIONS
#org.eclipse.ui/ENABLE_ANIMATIONS=true
org.eclipse.ui/KEY_CONFIGURATION_ID=com.apicloud.navigator.miner.accelerator

# 是否禁止左下角的Show View As a Fast View按钮
org.eclipse.ui/DISABLE_NEW_FAST_VIEW=false

org.eclipse.ui.editors/isWarningGoToNextNavigationTarget=false
org.eclipse.ui.editors/warningIndication=false
org.eclipse.ui.editors/warningIndicationInOverviewRuler=false
org.eclipse.ui.editors/warningIndicationInVerticalRuler=false